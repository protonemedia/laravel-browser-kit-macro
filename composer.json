{
    "name": "pbmedia/laravel-browser-kit-macro",
    "description": "A macro to use the Laravel 5.3 testing layer inside your Laravel 5.4+ tests",
    "keywords": [
        "protone-media",
        "laravel-browser-kit"
    ],
    "homepage": "https://github.com/protonemedia/laravel-browser-kit-macro",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "laravel/browser-kit-testing": "^6.0",
        "illuminate/http": "^8.67 || ^9.0",
        "illuminate/support": "^8.67 || ^9.0",
        "symfony/dom-crawler": "^5.1.14 || ^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23 || ^7.0",
        "phpunit/phpunit": "^9.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        }
    ],
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelBrowserKitMacro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelBrowserKitMacro\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelBrowserKitMacro\\BrowserKitMacroServiceProvider"
            ]
        }
    }
}